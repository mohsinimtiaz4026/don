{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/assets/*": ["src/assets/*"],
      "@/components/*": ["src/components/*"],
      "@/context/*": ["src/context/*"],
      "@/hooks/*": ["src/hooks/*"],
      "@/layouts/*": ["src/layouts/*"],
      "@/redux/*": ["src/redux/*"],
      "@/sections/*": ["src/sections/*"],
      "@/theme/*": ["src/theme/*"],
      "@/util/*": ["src/util/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "**/node_modules/*"]
}
